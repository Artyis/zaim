ServerSignature OFF
AddDefaultCharset UTF-8

ErrorDocument 404 /error.php

# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:zaim-zaym.ru
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?zaim-zaym.ru
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

RewriteCond %{THE_REQUEST} ([^/s]*)\/{2,}(\?[^/s]*)?
RewriteRule ^(.*) $ https://zaim-zaym.ru/$1 [R=301,L]
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:zaim-zaym.ru
